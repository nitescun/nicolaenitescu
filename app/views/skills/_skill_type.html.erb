<!--Skill Table-->
<div class="row">
  <div class="col s12 m10 offset-m1 l8 offset-l2" id="<%= skills.first.skill_type.downcase %>">
    <div class="card orange">
      <div class="card-content white-text">
        <div class="card-title center">
          <%= skills.first.skill_type %>
        </div>
        <table class="highlight centered">
          <thead>
            <tr>
              <th>Skill</th>
              <th>Proficiency</th>
            </tr>
          </thead>

          <tbody>
            <% skills.each do |skill| %>
              <tr>
                <td><%= skill.skill %></td>
                <td><%= gen_stars(skill.rating) %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>